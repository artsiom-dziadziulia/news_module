    <update handle="customer_account"/>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_account_edit_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Aw\Test\Block\Account\News" name="customer_edit" template="Aw_Test::form/news.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
